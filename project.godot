GlobalEffects="*res://scripts/autoloads/effects/global_effects.tscn"
GlobalDayNightClock="*res://scripts/autoloads/general/global_day_night_clock.tscn"
LimboConsole="*res://addons/limbo_console/limbo_console.gd"

[display]

window/stretch/mode="viewport"

[dotnet]

project/assembly_name="New Template"

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/dialogue_manager/plugin.cfg", "res://addons/limbo_console/plugin.cfg", "res://addons/shaker/plugin.cfg", "res://addons/ultimate_character/plugin.cfg")

