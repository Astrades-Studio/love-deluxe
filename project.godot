config_version=5

[application]

config/name="Template 2025"
config/features=PackedStringArray("4.4", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
Shaker="*res://addons/shaker/src/Shaker.gd"
MusicManager="*res://scripts/autoloads/audio/music_manager.gd"
AudioManager="*res://scripts/autoloads/audio/audio_manager.gd"
SoundPool="*res://scripts/autoloads/audio/sound_pool.gd"
ObjectPoolManager="*res://scripts/autoloads/behaviour/object_pool/object_pool_manager.gd"
GamepadControllerManager="*res://scripts/autoloads/gamepad/gamepad_controller_manager.gd"
GameGlobals="*res://scripts/autoloads/general/game_globals.gd"
GlobalGameEvents="*res://scripts/autoloads/general/global_game_events.gd"
SaveManager="*res://scripts/autoloads/persistence/save/save_manager.gd"
SettingsManager="*res://scripts/autoloads/persistence/settings/settings_manager.gd"
CursorManager="*res://scripts/autoloads/screen/cursor_manager.gd"
WindowManager="*res://scripts/autoloads/viewport/window_manager.gd"
SceneTransitionManager="*res://scripts/autoloads/scenes/scene_transition_manager.tscn"
